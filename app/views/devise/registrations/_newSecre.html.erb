<div>
  <h2 class="title-log"><b>Registrar Secretaria</b></h2>
  <div class="container-iniciar-form width-form sign-up">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="field">
        <%= f.label :first_name %><br />
        <%= f.text_field :first_name, autofocus: true, class:"form-control2" %>
      </div>

      <div class="field">
        <%= f.label :last_name %><br />
        <%= f.text_field :last_name, autofocus: true, class:"form-control2" %>
      </div>


      <div class="field">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true, class:"form-control2" %>
      </div>

      <div class="field">
        <%= f.label :password, "ContraseÃ±a" %>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> caracteres minimo)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off", class:"form-control2" %>
      </div>

      <div class="field">
        <%= f.label :password_confirmation, "Confirmar ContraseÃ±a" %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off", class:"form-control2" %>
      </div>

      <div class="actions">
        <%= f.submit "Crear cuenta", class:"btn btn-success btn-log" %>
      </div>
    <% end %>
  </div>
</div>
