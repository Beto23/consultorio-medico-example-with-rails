<div class="container">
  <section class="container-listado">
      <div class="container-all-listado">
        <h2>Secretarias</h2>
        <hr>
        <table class="table table-striped container-all-listado-table">
          <thead>
            <tr>
              <th>#</th>
              <th>Name</th>
              <th>Last name</th>
              <th>Email</th>
              <th>Sign in count</th>
            </tr>
          </thead>
          <tbody>
            <% @users.each do |user| %>
              <tr>
                <td><%= user.id  %></td>
                <td><%= user.first_name %></td>
                <td><%= user.last_name %></td>
                <td><%=user.email%></td>
                <td><%=user.sign_in_count %></td>
              </tr>
            <%end %>
          </tbody>
        </table>
      </div>
      <%= link_to (new_user_registration_path) do  %>
        <button type="button" class="btn btn-primary" name="button">Agregar </button>
      <%end %>
  </section>

</div>
