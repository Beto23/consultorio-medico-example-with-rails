<p id="notice"><%= notice %></p>

<h1>Listing Consultations</h1>

<table>
  <thead>
    <tr>
      <th>Time</th>
      <th>Fecha</th>
      <th>Medic</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consultations.each do |consultation| %>
      <tr>
        <td><%= consultation.time %></td>
        <td><%= consultation.fecha %></td>
        <td><%= consultation.medic %></td>
        <td><%= consultation.patient %></td>
        <td><%= link_to 'Show', consultation %></td>
        <td><%= link_to 'Edit', edit_consultation_path(consultation) %></td>
        <td><%= link_to 'Destroy', consultation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 <%= render "partials/modal" %>
<br>

<%= link_to 'New Consultation', new_consultation_path %>
