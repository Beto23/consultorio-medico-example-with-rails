<section class="container-listado">
  <ul class="nav nav-tabs">
    <li role="presentation" class="active" ><a href="">Especialidades</a></li>
  </ul>
    <div class="container-all-listado">
      <table class="table table-hover container-all-listado-table">
        <thead>
          <tr>
            <th>Id</th>
            <th>Especialidad</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @specialties.each do |specialty|  %>
            <tr>
              <td>#<%= specialty.id  %></td>
              <td><%= specialty.name  %></td>
              <td>
                <i class="fa fa-ban"></i>
                <i class="fa fa-pencil"></i>
              </td>
            </tr>
          <%end %>
        </tbody>
      </table>
    </div>
  <button type="button" class="btn btn-primary" name="button">Agregar Especialidad</button>
</section>
